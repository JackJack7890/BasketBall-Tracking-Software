{
  "name": "basketballtrackingnew",
  "productName": "Basketball Tracking New",
  "description": "My Electron application description",
  "version": "1.0.0",
  "author": "kanany",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -m"
  },
  "dependencies": {
  },
  "devDependencies": {
    "electron": "^29.1.5",
    "electron-builder": "^23.0.0"
  },
  "build": {
    "appId": "com.kanany.basketballtrackingnew",
    "productName": "Basketball Tracking New",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.code-workspace",
      "!**/*.js.map",
      "!**/*.tsbuildinfo",
      "!**/tsconfig.json",
      "!**/tslint.json",
      "!**/*.spec.ts",
      "!**/*.test.ts",
      "!**/*.spec.js",
      "!**/*.test.js",
      "!test/",
      "!tests/",
      "!spec/",
      "!**/__tests__/"
    ],
    "win": {
      "icon": "icon.ico"
    },
    "mac": {
      "icon": "../basketballLogo.icns"
    },
    "linux": {
      "icon": "icons/"
    }
  }
}
